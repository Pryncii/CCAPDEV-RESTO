

<div class = "flexmain">
    {{> header}}
    
    <div class = "headerright">
        <button type="logout-btn" class = "logout-btn"  onclick="window.location.href='/'">Logout</button>
        <a href="/profile-page/{{user.urlname}}"><img src = "{{user.image}}"></a>
    </div>
</div>
<div class="flexrow headerholder"></div>
<div class="inside">
    {{> sidebar}}
    <div class="contentforotherpages">
    <div class = "flexbody">
        {{#each restos}}
        <div class = "flexrestopart flexmenulist">
            {{#each this}}
            <div class = "flexmenuresto">
                <span>{{name}}</span>
                <a href = "/restaurant/{{landmark}}/{{linkname}}">
                <img src = "{{image}}">
                </a>
            </div>
            {{/each}}
        </div>{{/each}}
        
    </div>   
</div>
</div>
