<div class = "flexmain">
    {{> header}}
    
    <div class = "headerright">
        <button type="logout-btn" class = "logout-btn"  onclick="window.location.href='/'">Logout</button>
        {{#eq checkUser user.urlname}}
        <a href="/profile-page/{{user.urlname}}"><img src = "{{user.image}}"></a>
        {{/eq}}
        {{#eq checkUser user.linkname}}
        <a href="/showall/"><img src = "{{user.imagesquare}}"></a>
        {{/eq}}
    </div>
</div>
</div>
<div class="headerholder"></div>
<div class="inside">
    {{> sidebar}}
  <div class="main">
    <div class="{{status}}">
      <h1>{{msg}}</h1>
    </div>
  </div>
</div>
